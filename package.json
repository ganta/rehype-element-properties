    "fix:prettier": "prettier --write '**/*.{ts,json,json5,cjs}'",
    "fix:eslint": "eslint '**/*.ts' --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganta/rehype-element-properties.git"
