    "unist-util-visit": "~4.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@jest/types": "27.5.1",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "babel-jest": "27.5.1",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.5.1",
    "microbundle": "0.14.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.0",
    "rehype": "12.0.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.2",
    "unified": "10.1.2"
