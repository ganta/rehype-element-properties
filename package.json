{
  "name": "@ganta/rehype-element-properties",
  "version": "1.0.0",
  "description": "Rehype plugin to replace element properties",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "microbundle watch",
    "build": "microbundle",
